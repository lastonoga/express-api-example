{
  "name": "express-test",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/express": "^5",
    "@types/http-errors": "^2.0.4",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "tsconfig-paths": "^4.2.0"
  },
  "scripts": {
    "dev": "tsx watch --tsconfig tsconfig.json -r tsconfig-paths/register index.ts",
    "build": "yarn build:docs",
    "build:docs": "tsx ./scripts/swagger.ts",
    "test": "vitest run --globals index.spec.ts",
    "validate": "vitest run --globals validate.spec.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "database:migrate": "prisma migrate dev --name init_user_model"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "compression": "^1.8.0",
    "express": "^5",
    "express-rate-limit": "^7.5.0",
    "express-zod-api": "^22.11.2",
    "http-errors": "^2.0.0",
    "module-alias": "^2.2.3",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}